<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="150.0" prefWidth="461.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aseguradora.ErrorPopupController">
    <children>
      <Pane layoutX="2.0" prefHeight="161.0" prefWidth="459.0">
         <children>
                <Button layoutX="368.0" layoutY="124.0" mnemonicParsing="false" onAction="#closeErrorPopup" text="Aceptar" />
                <Label fx:id="errorMessageLabel" layoutX="122.0" layoutY="70.0" text="Mensaje de error" />
            <ImageView fx:id="imagenView" fitHeight="55.0" fitWidth="57.0" layoutX="37.0" layoutY="51.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@error.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
    </children>
</AnchorPane>
