<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aseguradora.ErrorPopupController">
    <children>
        <VBox alignment="CENTER" prefHeight="150.0" prefWidth="300.0" spacing="10.0">
            <children>
                <Label fx:id="errorMessageLabel" text="Mensaje de error" />
                <Button mnemonicParsing="false" onAction="#closeErrorPopup" text="Cerrar" />
            </children>
        </VBox>
    </children>
</AnchorPane>
